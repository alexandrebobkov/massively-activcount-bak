{{!< default}}

{{#post}}
{{!-- SERVICES page template --}}
{{!-- Everything inside the #post tags pulls data from the page --}}

<main id="main">
    <section class="post">

        {{#if feature_image}}
        <figure class="image main">
            <img src="{{img_url feature_image}}" alt="{{#if feature_image_alt}}{{feature_image_alt}}{{else}}{{title}}{{/if}}" />
        </figure>
        {{/if}}

        <div class="content">
            {{content}}
        </div>

    </section>
</main>

{{/post}}